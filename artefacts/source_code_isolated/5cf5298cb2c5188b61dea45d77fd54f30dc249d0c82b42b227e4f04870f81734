public static String getNetworkTypeName(int type){
    switch(type) {
        case TYPE_NONE:
            return "NONE";
        case TYPE_MOBILE:
            return "MOBILE";
        case TYPE_WIFI:
            return "WIFI";
        case TYPE_MOBILE_MMS:
            return "MOBILE_MMS";
        case TYPE_MOBILE_SUPL:
            return "MOBILE_SUPL";
        case TYPE_MOBILE_DUN:
            return "MOBILE_DUN";
        case TYPE_MOBILE_HIPRI:
            return "MOBILE_HIPRI";
        case TYPE_WIMAX:
            return "WIMAX";
        case TYPE_BLUETOOTH:
            return "BLUETOOTH";
        case TYPE_DUMMY:
            return "DUMMY";
        case TYPE_ETHERNET:
            return "ETHERNET";
        case TYPE_MOBILE_FOTA:
            return "MOBILE_FOTA";
        case TYPE_MOBILE_IMS:
            return "MOBILE_IMS";
        case TYPE_MOBILE_CBS:
            return "MOBILE_CBS";
        case TYPE_WIFI_P2P:
            return "WIFI_P2P";
        case TYPE_MOBILE_IA:
            return "MOBILE_IA";
        case TYPE_MOBILE_EMERGENCY:
            return "MOBILE_EMERGENCY";
        case TYPE_PROXY:
            return "PROXY";
        case TYPE_VPN:
            return "VPN";
        default:
            return Integer.toString(type);
    }
}