public Image acquireLatestImage(){
    Image image = acquireNextImage();
    if (image == null) {
        return null;
    }
    try {
        for (; ; ) {
            Image next = acquireNextImageNoThrowISE();
            if (next == null) {
                Image result = image;
                image = null;
                return result;
            }
            image.close();
            image = next;
        }
    } finally {
        if (image != null) {
            image.close();
        }
    }
}