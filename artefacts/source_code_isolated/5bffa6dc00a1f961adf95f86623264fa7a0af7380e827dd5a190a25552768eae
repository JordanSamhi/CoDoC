public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    PrinterInfo other = (PrinterInfo) obj;
    if (!equalsIgnoringStatus(other)) {
        return false;
    }
    if (mStatus != other.mStatus) {
        return false;
    }
    return true;
}