public boolean canApplyTheme(){
    if (mThemeAttrs != null) {
        return true;
    }
    boolean fillCanApplyTheme = canComplexColorApplyTheme(mFillColors);
    boolean strokeCanApplyTheme = canComplexColorApplyTheme(mStrokeColors);
    if (fillCanApplyTheme || strokeCanApplyTheme) {
        return true;
    }
    return false;
}