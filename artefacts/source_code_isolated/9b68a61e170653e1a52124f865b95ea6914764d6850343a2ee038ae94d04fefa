public static byte[] hexStringToBytes(String s){
    byte[] ret;
    if (s == null)
        return null;
    int sz = s.length();
    ret = new byte[sz / 2];
    for (int i = 0; i < sz; i += 2) {
        ret[i / 2] = (byte) ((hexCharToInt(s.charAt(i)) << 4) | hexCharToInt(s.charAt(i + 1)));
    }
    return ret;
}