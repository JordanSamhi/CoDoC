public String toString(){
    String result = mDeviceName + " (" + mDeviceAddress + ")";
    if (mDeviceAlias != null) {
        result += ", alias " + mDeviceAlias;
    }
    result += ", isAvailable " + mIsAvailable + ", canConnect " + mCanConnect + ", isRemembered " + mIsRemembered;
    return result;
}