public String toString(){
    String str = new String("Equalizer" + ";curPreset=" + Short.toString(curPreset) + ";numBands=" + Short.toString(numBands));
    for (int i = 0; i < numBands; i++) {
        str = str.concat(";band" + (i + 1) + "Level=" + Short.toString(bandLevels[i]));
    }
    return str;
}