public String toString(){
    return "RouteInfo{ id=" + id + ", name=" + name + ", description=" + description + ", supportedTypes=0x" + Integer.toHexString(supportedTypes) + ", enabled=" + enabled + ", statusCode=" + statusCode + ", playbackType=" + playbackType + ", playbackStream=" + playbackStream + ", volume=" + volume + ", volumeMax=" + volumeMax + ", volumeHandling=" + volumeHandling + ", presentationDisplayId=" + presentationDisplayId + ", deviceType=" + deviceType + " }";
}