public int hashCode(){
    final int prime = 31;
    int result = 1;
    result = prime * result + ((mName != null) ? mName.hashCode() : 0);
    result = prime * result + mContentType;
    result = prime * result + mPageCount;
    result = prime * result + (int) mDataSize;
    result = prime * result + (int) (mDataSize >> 32);
    return result;
}