public static String priorityToString(@Priority int pri){
    switch(pri) {
        case PRIORITY_MIN:
            return "MIN";
        case PRIORITY_LOW:
            return "LOW";
        case PRIORITY_DEFAULT:
            return "DEFAULT";
        case PRIORITY_HIGH:
            return "HIGH";
        case PRIORITY_MAX:
            return "MAX";
        default:
            return "UNKNOWN(" + String.valueOf(pri) + ")";
    }
}