public static long resetLockedMeta(long state){
    if ((state & META_CAP_LOCKED) != 0) {
        state &= ~META_SHIFT_MASK;
    }
    if ((state & META_ALT_LOCKED) != 0) {
        state &= ~META_ALT_MASK;
    }
    if ((state & META_SYM_LOCKED) != 0) {
        state &= ~META_SYM_MASK;
    }
    return state;
}