public boolean equals(final Object obj){
    if (obj == null) {
        return false;
    }
    if (this == obj) {
        return true;
    }
    if (obj instanceof ReprocessFormatsMap) {
        final ReprocessFormatsMap other = (ReprocessFormatsMap) obj;
        return Arrays.equals(mEntry, other.mEntry);
    }
    return false;
}