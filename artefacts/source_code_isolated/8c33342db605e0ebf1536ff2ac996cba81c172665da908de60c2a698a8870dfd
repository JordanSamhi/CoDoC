public void set(int uid, String name){
    if (name == null) {
        throw new NullPointerException("Name can't be null");
    }
    mNum = 1;
    if (mUids == null) {
        mUids = new int[2];
        mNames = new String[2];
    }
    mUids[0] = uid;
    mNames[0] = name;
    if (mChains != null) {
        mChains.clear();
    }
}