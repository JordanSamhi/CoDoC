public Builder setChoices(@Nullable CharSequence[] choices){
    if (choices == null) {
        mChoices = null;
    } else {
        mChoices = new CharSequence[choices.length];
        for (int i = 0; i < choices.length; i++) {
            mChoices[i] = Notification.safeCharSequence(choices[i]);
        }
    }
    return this;
}