public void setDisabledSystemUiVisibility(int flags){
    if (mAttachInfo != null) {
        if (mAttachInfo.mDisabledSystemUiVisibility != flags) {
            mAttachInfo.mDisabledSystemUiVisibility = flags;
            if (mParent != null) {
                mParent.recomputeViewAttributes(this);
            }
        }
    }
}