public PointF evaluate(float fraction, PointF startValue, PointF endValue){
    float x = startValue.x + (fraction * (endValue.x - startValue.x));
    float y = startValue.y + (fraction * (endValue.y - startValue.y));
    if (mPoint != null) {
        mPoint.set(x, y);
        return mPoint;
    } else {
        return new PointF(x, y);
    }
}