public boolean equals(Object obj){
    if (obj == null) {
        return false;
    } else if (this == obj) {
        return true;
    } else if (obj instanceof Range) {
        @SuppressWarnings("rawtypes")
        Range other = (Range) obj;
        return mLower.equals(other.mLower) && mUpper.equals(other.mUpper);
    }
    return false;
}