public boolean equals(Object o){
    if (this == o)
        return true;
    if (o == null || !(o instanceof InputDeviceIdentifier))
        return false;
    final InputDeviceIdentifier that = (InputDeviceIdentifier) o;
    return ((mVendorId == that.mVendorId) && (mProductId == that.mProductId) && TextUtils.equals(mDescriptor, that.mDescriptor));
}