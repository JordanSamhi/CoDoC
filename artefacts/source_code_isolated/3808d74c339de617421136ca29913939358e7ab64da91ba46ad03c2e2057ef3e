public long computeBatteryTimeRemaining(long curTime){
    if (!mOnBattery) {
        return -1;
    }
    if (mDischargeStepTracker.mNumStepDurations < 1) {
        return -1;
    }
    long msPerLevel = mDischargeStepTracker.computeTimePerLevel();
    if (msPerLevel <= 0) {
        return -1;
    }
    return (msPerLevel * mCurrentBatteryLevel) * 1000;
}