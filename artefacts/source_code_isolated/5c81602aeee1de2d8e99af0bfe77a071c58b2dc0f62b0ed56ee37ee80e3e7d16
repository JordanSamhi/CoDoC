public int hashCode(){
    final int prime = 31;
    int result = 1;
    long temp;
    temp = Double.doubleToLongBits(mLatitude);
    result = prime * result + (int) (temp ^ (temp >>> 32));
    temp = Double.doubleToLongBits(mLongitude);
    result = prime * result + (int) (temp ^ (temp >>> 32));
    result = prime * result + Float.floatToIntBits(mRadius);
    result = prime * result + mType;
    return result;
}