public void dispatchPause(boolean finishing){
    if (finishing) {
        mFinishing = true;
    }
    mCurState = STARTED;
    if (mSingleMode) {
        if (mResumed != null) {
            moveToState(mResumed, STARTED);
        }
    } else {
        final int N = mActivityArray.size();
        for (int i = 0; i < N; i++) {
            LocalActivityRecord r = mActivityArray.get(i);
            if (r.curState == RESUMED) {
                moveToState(r, STARTED);
            }
        }
    }
}