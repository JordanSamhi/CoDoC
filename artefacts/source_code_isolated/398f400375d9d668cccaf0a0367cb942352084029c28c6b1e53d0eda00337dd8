public String toString(){
    StringBuilder b = new StringBuilder(String.format("NdefRecord tnf=%X", mTnf));
    if (mType.length > 0)
        b.append(" type=").append(bytesToString(mType));
    if (mId.length > 0)
        b.append(" id=").append(bytesToString(mId));
    if (mPayload.length > 0)
        b.append(" payload=").append(bytesToString(mPayload));
    return b.toString();
}