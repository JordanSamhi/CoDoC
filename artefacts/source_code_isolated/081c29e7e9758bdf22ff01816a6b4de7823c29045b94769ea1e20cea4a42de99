public static String qualityToString(int quality){
    switch(quality) {
        case ACCURACY_FINE:
            return "ACCURACY_FINE";
        case ACCURACY_BLOCK:
            return "ACCURACY_BLOCK";
        case ACCURACY_CITY:
            return "ACCURACY_CITY";
        case POWER_NONE:
            return "POWER_NONE";
        case POWER_LOW:
            return "POWER_LOW";
        case POWER_HIGH:
            return "POWER_HIGH";
        default:
            return "???";
    }
}