public boolean equals(Object object){
    if (this == object) {
        return true;
    }
    if (object == null) {
        return false;
    }
    if (getClass() != object.getClass()) {
        return false;
    }
    PrinterId other = (PrinterId) object;
    if (!mServiceName.equals(other.mServiceName)) {
        return false;
    }
    if (!mLocalId.equals(other.mLocalId)) {
        return false;
    }
    return true;
}