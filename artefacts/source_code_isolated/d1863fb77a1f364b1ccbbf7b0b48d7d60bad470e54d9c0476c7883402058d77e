public boolean equals(@Nullable Object o){
    if (!(o instanceof HdmiPortInfo)) {
        return false;
    }
    final HdmiPortInfo other = (HdmiPortInfo) o;
    return mId == other.mId && mType == other.mType && mAddress == other.mAddress && mCecSupported == other.mCecSupported && mArcSupported == other.mArcSupported && mMhlSupported == other.mMhlSupported;
}