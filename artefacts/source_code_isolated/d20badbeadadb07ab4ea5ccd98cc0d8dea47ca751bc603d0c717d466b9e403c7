public static TextLine obtain(){
    TextLine tl;
    synchronized (sCached) {
        for (int i = sCached.length; --i >= 0; ) {
            if (sCached[i] != null) {
                tl = sCached[i];
                sCached[i] = null;
                return tl;
            }
        }
    }
    tl = new TextLine();
    if (DEBUG) {
        Log.v("TLINE", "new: " + tl);
    }
    return tl;
}