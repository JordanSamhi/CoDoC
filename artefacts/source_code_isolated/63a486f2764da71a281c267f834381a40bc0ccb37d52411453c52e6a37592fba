public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (!(obj instanceof Pin)) {
        return false;
    }
    Pin other = (Pin) obj;
    if (other.hashCode() != mHashCode) {
        return false;
    }
    if (!Arrays.equals(digest, other.digest)) {
        return false;
    }
    if (!digestAlgorithm.equals(other.digestAlgorithm)) {
        return false;
    }
    return true;
}