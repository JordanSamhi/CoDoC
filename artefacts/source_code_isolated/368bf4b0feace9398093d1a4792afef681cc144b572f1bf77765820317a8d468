public void reverse(){
    if (!mIsReversible || !mInitialized) {
        return;
    }
    if (useLastSeenTarget()) {
        if (DBG_ANIMATION_VECTOR_DRAWABLE) {
            Log.d(LOGTAG, "Target is set. Reversing VDAnimatorSet from java");
        }
        reverseAnimation();
    } else {
        addPendingAction(REVERSE_ANIMATION);
    }
}