public void start(){
    if (!mInitialized) {
        return;
    }
    if (useLastSeenTarget()) {
        if (DBG_ANIMATION_VECTOR_DRAWABLE) {
            Log.d(LOGTAG, "Target is set. Starting VDAnimatorSet from java");
        }
        startAnimation();
    } else {
        addPendingAction(START_ANIMATION);
    }
}