public boolean equals(Object o){
    if (o instanceof DisconnectCause) {
        DisconnectCause d = (DisconnectCause) o;
        return Objects.equals(mDisconnectCode, d.getCode()) && Objects.equals(mDisconnectLabel, d.getLabel()) && Objects.equals(mDisconnectDescription, d.getDescription()) && Objects.equals(mDisconnectReason, d.getReason()) && Objects.equals(mToneToPlay, d.getTone());
    }
    return false;
}