public String toString(){
    String port = "";
    if ((mPort != 443 && mScheme.equals("https")) || (mPort != 80 && mScheme.equals("http"))) {
        port = ":" + Integer.toString(mPort);
    }
    String authInfo = "";
    if (mAuthInfo.length() > 0) {
        authInfo = mAuthInfo + "@";
    }
    return mScheme + "://" + authInfo + mHost + port + mPath;
}