public void toShortSummaryString(StringBuilder b){
    if (mHtmlText != null) {
        b.append("HTML");
    } else if (mText != null) {
        b.append("TEXT");
    } else if (mUri != null) {
        b.append("U:");
        b.append(mUri);
    } else if (mIntent != null) {
        b.append("I:");
        mIntent.toShortString(b, true, true, true, true);
    } else {
        b.append("NULL");
    }
}