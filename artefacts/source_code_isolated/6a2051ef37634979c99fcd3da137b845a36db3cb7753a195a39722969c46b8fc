public static String toString(int state){
    switch(state) {
        case IDLE:
            return "IDLE";
        case INITIATED:
            return "INITIATED";
        case NEGOTIATING:
            return "NEGOTIATING";
        case ESTABLISHING:
            return "ESTABLISHING";
        case ESTABLISHED:
            return "ESTABLISHED";
        case RENEGOTIATING:
            return "RENEGOTIATING";
        case REESTABLISHING:
            return "REESTABLISHING";
        case TERMINATING:
            return "TERMINATING";
        case TERMINATED:
            return "TERMINATED";
        default:
            return "UNKNOWN";
    }
}