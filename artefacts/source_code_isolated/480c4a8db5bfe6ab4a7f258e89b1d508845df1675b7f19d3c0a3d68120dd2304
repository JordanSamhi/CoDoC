public File chooseForRead(){
    File result;
    if (mReal.exists()) {
        result = mReal;
        if (mTemp.exists()) {
            mTemp.delete();
        }
    } else if (mTemp.exists()) {
        result = mTemp;
        mTemp.renameTo(mReal);
    } else {
        return mReal;
    }
    return result;
}