public int indexOf(Object o){
    ListIterator<E> it = listIterator();
    if (o == null) {
        while (it.hasNext()) if (it.next() == null)
            return it.previousIndex();
    } else {
        while (it.hasNext()) if (o.equals(it.next()))
            return it.previousIndex();
    }
    return -1;
}