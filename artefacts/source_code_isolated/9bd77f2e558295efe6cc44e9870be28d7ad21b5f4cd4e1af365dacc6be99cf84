public static BigInteger valueOf(long value){
    if (value < 0) {
        if (value != -1) {
            return new BigInteger(-1, -value);
        }
        return MINUS_ONE;
    } else if (value < SMALL_VALUES.length) {
        return SMALL_VALUES[(int) value];
    } else {
        return new BigInteger(1, value);
    }
}