public void beginUpdate(CharSequence label){
    if (mInUndo) {
        throw new IllegalStateException("Can't being update while performing undo/redo");
    }
    if (mUpdateCount <= 0) {
        createWorkingState();
        mMerged = false;
        mUpdateCount = 0;
    }
    mWorking.updateLabel(label);
    mUpdateCount++;
}