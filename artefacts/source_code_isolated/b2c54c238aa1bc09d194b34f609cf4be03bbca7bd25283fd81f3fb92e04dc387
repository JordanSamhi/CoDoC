public void onTuned(Uri channelUri){
    synchronized (mLock) {
        if (DEBUG) {
            Slog.d(TAG, "onTuned()");
        }
        if (mSessionState.session == null || mSessionState.client == null) {
            return;
        }
        try {
            mSessionState.client.onTuned(mSessionState.seq, channelUri);
        } catch (RemoteException e) {
            Slog.e(TAG, "error in onTuned", e);
        }
    }
}