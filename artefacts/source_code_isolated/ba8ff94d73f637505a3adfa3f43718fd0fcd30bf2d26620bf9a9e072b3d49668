public String toString(){
    String role = Integer.toString(mRole);
    switch(mRole) {
        case ROLE_NONE:
            role = "NONE";
            break;
        case ROLE_SOURCE:
            role = "SOURCE";
            break;
        case ROLE_SINK:
            role = "SINK";
            break;
    }
    return "{mHandle: " + mHandle + ", mRole: " + role + "}";
}