public boolean hasPermission(UsbAccessory accessory){
    try {
        return mService.hasAccessoryPermission(new android.hardware.usb.UsbAccessory(accessory.getManufacturer(), accessory.getModel(), accessory.getDescription(), accessory.getVersion(), accessory.getUri(), accessory.getSerial()));
    } catch (RemoteException e) {
        Log.e(TAG, "RemoteException in hasPermission", e);
        return false;
    }
}