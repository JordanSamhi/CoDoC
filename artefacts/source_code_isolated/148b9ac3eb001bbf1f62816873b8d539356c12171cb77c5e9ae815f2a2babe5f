public boolean addLinkAddress(@NonNull LinkAddress address){
    if (address == null) {
        return false;
    }
    int i = findLinkAddressIndex(address);
    if (i < 0) {
        mLinkAddresses.add(address);
        return true;
    } else if (mLinkAddresses.get(i).equals(address)) {
        return false;
    } else {
        mLinkAddresses.set(i, address);
        return true;
    }
}