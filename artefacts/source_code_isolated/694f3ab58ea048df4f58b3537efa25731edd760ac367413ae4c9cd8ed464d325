public void setYUVtoRGB(){
    mMatrix.loadIdentity();
    mMatrix.set(0, 0, 1.f);
    mMatrix.set(1, 0, 0.f);
    mMatrix.set(2, 0, 1.13983f);
    mMatrix.set(0, 1, 1.f);
    mMatrix.set(1, 1, -0.39465f);
    mMatrix.set(2, 1, -0.5806f);
    mMatrix.set(0, 2, 1.f);
    mMatrix.set(1, 2, 2.03211f);
    mMatrix.set(2, 2, 0.f);
    setMatrix();
}