public static int checkNativeType(int nativeType){
    switch(nativeType) {
        case TYPE_BYTE:
        case TYPE_INT32:
        case TYPE_FLOAT:
        case TYPE_INT64:
        case TYPE_DOUBLE:
        case TYPE_RATIONAL:
            return nativeType;
    }
    throw new UnsupportedOperationException("Unknown nativeType " + nativeType);
}