public long computeDurationHint(){
    long duration = 0;
    final int count = mAnimations.size();
    final ArrayList<Animation> animations = mAnimations;
    for (int i = count - 1; i >= 0; --i) {
        final long d = animations.get(i).computeDurationHint();
        if (d > duration)
            duration = d;
    }
    return duration;
}