public String toString(){
    String returnValue = "TransitionValues@" + Integer.toHexString(hashCode()) + ":\n";
    returnValue += "    view = " + view + "\n";
    returnValue += "    values:";
    for (String s : values.keySet()) {
        returnValue += "    " + s + ": " + values.get(s) + "\n";
    }
    return returnValue;
}