public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (!super.equals(obj)) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    AndroidKeyStorePublicKey other = (AndroidKeyStorePublicKey) obj;
    if (!Arrays.equals(mEncoded, other.mEncoded)) {
        return false;
    }
    return true;
}