private int getUid(){
    if (isUidEnumeration()) {
        if (mUidOrUidIndex < 0 || mUidOrUidIndex >= mUids.length) {
            throw new IndexOutOfBoundsException("Index=" + mUidOrUidIndex + " mUids.length=" + mUids.length);
        }
        return mUids[mUidOrUidIndex];
    }
    return mUidOrUidIndex;
}