public boolean getBoolean(int tag){
    if (KeymasterDefs.getTagType(tag) != KeymasterDefs.KM_BOOL) {
        throw new IllegalArgumentException("Not a boolean tag: " + tag);
    }
    KeymasterArgument arg = getArgumentByTag(tag);
    if (arg == null) {
        return false;
    }
    return true;
}