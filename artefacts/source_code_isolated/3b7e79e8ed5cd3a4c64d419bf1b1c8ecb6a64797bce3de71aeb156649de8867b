public void toShortString(StringBuilder b){
    if (mHtmlText != null) {
        b.append("H:");
        b.append(mHtmlText);
    } else if (mText != null) {
        b.append("T:");
        b.append(mText);
    } else if (mUri != null) {
        b.append("U:");
        b.append(mUri);
    } else if (mIntent != null) {
        b.append("I:");
        mIntent.toShortString(b, true, true, true, true);
    } else {
        b.append("NULL");
    }
}