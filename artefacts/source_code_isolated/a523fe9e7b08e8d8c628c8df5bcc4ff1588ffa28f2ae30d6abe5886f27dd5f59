public int[] getValueAsInts(){
    if (mValue == null) {
        return null;
    } else if (mValue instanceof long[]) {
        long[] val = (long[]) mValue;
        int[] arr = new int[val.length];
        for (int i = 0; i < val.length; i++) {
            arr[i] = (int) val[i];
        }
        return arr;
    }
    return null;
}