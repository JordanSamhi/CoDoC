public int getAsuLevel(){
    int lteAsuLevel = 99;
    int lteDbm = mRsrp;
    if (lteDbm == CellInfo.UNAVAILABLE)
        lteAsuLevel = 99;
    else if (lteDbm <= -140)
        lteAsuLevel = 0;
    else if (lteDbm >= -43)
        lteAsuLevel = 97;
    else
        lteAsuLevel = lteDbm + 140;
    if (DBG)
        log("Lte Asu level: " + lteAsuLevel);
    return lteAsuLevel;
}