public boolean equals(Object o){
    if (o == null || !(o instanceof AudioDevicePort)) {
        return false;
    }
    AudioDevicePort other = (AudioDevicePort) o;
    if (mType != other.type()) {
        return false;
    }
    if (mAddress == null && other.address() != null) {
        return false;
    }
    if (!mAddress.equals(other.address())) {
        return false;
    }
    return super.equals(o);
}