public boolean equals(@Nullable Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    PrintServiceInfo other = (PrintServiceInfo) obj;
    if (mId == null) {
        if (other.mId != null) {
            return false;
        }
    } else if (!mId.equals(other.mId)) {
        return false;
    }
    return true;
}