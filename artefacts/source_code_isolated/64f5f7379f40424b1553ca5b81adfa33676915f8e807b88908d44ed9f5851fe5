public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    ActivityResolveInfo other = (ActivityResolveInfo) obj;
    if (Float.floatToIntBits(weight) != Float.floatToIntBits(other.weight)) {
        return false;
    }
    return true;
}