public void set(int second, int minute, int hour, int monthDay, int month, int year){
    this.allDay = false;
    this.second = second;
    this.minute = minute;
    this.hour = hour;
    this.monthDay = monthDay;
    this.month = month;
    this.year = year;
    this.weekDay = 0;
    this.yearDay = 0;
    this.isDst = -1;
    this.gmtoff = 0;
}