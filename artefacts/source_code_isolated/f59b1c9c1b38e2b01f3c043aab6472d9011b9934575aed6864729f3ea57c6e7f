public BigInteger toBigInteger(){
    if ((scale == 0) || (isZero())) {
        return getUnscaledValue();
    } else if (scale < 0) {
        return getUnscaledValue().multiply(Multiplication.powerOf10(-(long) scale));
    } else {
        return getUnscaledValue().divide(Multiplication.powerOf10(scale));
    }
}