public String getMessage(){
    if (file == null && other == null)
        return getReason();
    StringBuilder sb = new StringBuilder();
    if (file != null)
        sb.append(file);
    if (other != null) {
        sb.append(" -> ");
        sb.append(other);
    }
    if (getReason() != null) {
        sb.append(": ");
        sb.append(getReason());
    }
    return sb.toString();
}