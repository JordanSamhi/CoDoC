public int[] getNearestKeys(int x, int y){
    if (mGridNeighbors == null)
        computeNearestNeighbors();
    if (x >= 0 && x < getMinWidth() && y >= 0 && y < getHeight()) {
        int index = (y / mCellHeight) * GRID_WIDTH + (x / mCellWidth);
        if (index < GRID_SIZE) {
            return mGridNeighbors[index];
        }
    }
    return new int[0];
}