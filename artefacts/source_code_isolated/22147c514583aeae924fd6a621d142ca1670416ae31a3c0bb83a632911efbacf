public int get(int i){
    switch(i) {
        case 0:
            return (int) (x);
        case 1:
            return (int) (y);
        case 2:
            return (int) (z);
        case 3:
            return (int) (w);
        default:
            throw new IndexOutOfBoundsException("Index: i");
    }
}