public ArrayList<PolicyInfo> getUsedPolicies(){
    ArrayList<PolicyInfo> res = new ArrayList<PolicyInfo>();
    for (int i = 0; i < sPoliciesDisplayOrder.size(); i++) {
        PolicyInfo pi = sPoliciesDisplayOrder.get(i);
        if (usesPolicy(pi.ident)) {
            res.add(pi);
        }
    }
    return res;
}