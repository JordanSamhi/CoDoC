public static RangeInfo obtain(int type, float min, float max, float current){
    RangeInfo info = sPool.acquire();
    if (info == null) {
        return new RangeInfo(type, min, max, current);
    }
    info.mType = type;
    info.mMin = min;
    info.mMax = max;
    info.mCurrent = current;
    return info;
}