public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    PageRange other = (PageRange) obj;
    if (mEnd != other.mEnd) {
        return false;
    }
    if (mStart != other.mStart) {
        return false;
    }
    return true;
}