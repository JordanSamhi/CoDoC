public boolean equals(@Nullable Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    PreciseCallState other = (PreciseCallState) obj;
    return (mRingingCallState == other.mRingingCallState && mForegroundCallState == other.mForegroundCallState && mBackgroundCallState == other.mBackgroundCallState && mDisconnectCause == other.mDisconnectCause && mPreciseDisconnectCause == other.mPreciseDisconnectCause);
}