public boolean equals(Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    PointF pointF = (PointF) o;
    if (Float.compare(pointF.x, x) != 0)
        return false;
    if (Float.compare(pointF.y, y) != 0)
        return false;
    return true;
}