public void removeRoute(RouteInfo route){
    if (route.getGroup() != this) {
        throw new IllegalArgumentException("Route " + route + " is not a member of this group.");
    }
    mRoutes.remove(route);
    route.mGroup = null;
    mUpdateName = true;
    updateVolume();
    dispatchRouteUngrouped(route, this);
    routeUpdated();
}