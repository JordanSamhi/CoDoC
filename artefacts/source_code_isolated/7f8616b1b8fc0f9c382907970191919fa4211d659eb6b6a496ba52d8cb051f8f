public boolean onTouchEvent(MotionEvent event){
    boolean retValue = mGestureDetector.onTouchEvent(event);
    int action = event.getAction();
    if (action == MotionEvent.ACTION_UP) {
        onUp();
    } else if (action == MotionEvent.ACTION_CANCEL) {
        onCancel();
    }
    return retValue;
}