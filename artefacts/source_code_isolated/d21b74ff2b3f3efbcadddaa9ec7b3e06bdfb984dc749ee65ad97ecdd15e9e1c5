public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    DataConnectionRealTimeInfo other = (DataConnectionRealTimeInfo) obj;
    return (mTime == other.mTime) && (mDcPowerState == other.mDcPowerState);
}