public String toString(){
    if (size() <= 0) {
        return "{}";
    }
    StringBuilder buffer = new StringBuilder(mSize * 28);
    buffer.append('{');
    for (int i = 0; i < mSize; i++) {
        if (i > 0) {
            buffer.append(", ");
        }
        int key = keyAt(i);
        buffer.append(key);
        buffer.append('=');
        int value = valueAt(i);
        buffer.append(value);
    }
    buffer.append('}');
    return buffer.toString();
}