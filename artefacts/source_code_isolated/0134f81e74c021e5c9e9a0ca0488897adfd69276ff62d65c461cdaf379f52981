public boolean containsKey(Object key){
    Iterator<Map.Entry<K, V>> i = entrySet().iterator();
    if (key == null) {
        while (i.hasNext()) {
            Entry<K, V> e = i.next();
            if (e.getKey() == null)
                return true;
        }
    } else {
        while (i.hasNext()) {
            Entry<K, V> e = i.next();
            if (key.equals(e.getKey()))
                return true;
        }
    }
    return false;
}