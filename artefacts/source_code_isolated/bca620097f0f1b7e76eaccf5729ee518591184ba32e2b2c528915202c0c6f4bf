public ArrayList<SyncHistoryItem> getSyncHistory(){
    synchronized (mAuthorities) {
        final int N = mSyncHistory.size();
        ArrayList<SyncHistoryItem> items = new ArrayList<SyncHistoryItem>(N);
        for (int i = 0; i < N; i++) {
            items.add(mSyncHistory.get(i));
        }
        return items;
    }
}