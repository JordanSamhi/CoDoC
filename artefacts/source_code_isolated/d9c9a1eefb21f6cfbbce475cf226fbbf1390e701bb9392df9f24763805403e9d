public String getCdmaMdn(int subId){
    try {
        ITelephony telephony = getITelephony();
        if (telephony == null)
            return null;
        return telephony.getCdmaMdn(subId);
    } catch (RemoteException ex) {
        return null;
    } catch (NullPointerException ex) {
        return null;
    }
}