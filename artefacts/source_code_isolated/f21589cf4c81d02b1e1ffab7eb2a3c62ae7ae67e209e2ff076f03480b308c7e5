public RequestHolder previewProduced(){
    final ReentrantLock lock = this.mLock;
    lock.lock();
    try {
        CaptureHolder h = mPreviewProduceQueue.poll();
        if (h == null) {
            Log.w(TAG, "previewProduced called with no preview request on queue!");
            return null;
        }
        h.setPreviewProduced();
        return h.mRequest;
    } finally {
        lock.unlock();
    }
}