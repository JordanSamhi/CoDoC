public void set(int monthDay, int month, int year){
    this.allDay = true;
    this.second = 0;
    this.minute = 0;
    this.hour = 0;
    this.monthDay = monthDay;
    this.month = month;
    this.year = year;
    this.weekDay = 0;
    this.yearDay = 0;
    this.isDst = -1;
    this.gmtoff = 0;
}