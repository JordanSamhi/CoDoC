public String toShortString(){
    StringBuilder sb = new StringBuilder();
    sb.append(Integer.toHexString(System.identityHashCode(this)));
    sb.append(" #");
    UserHandle.formatUid(sb, callingUid);
    sb.append("/");
    sb.append(job.getId());
    sb.append(' ');
    sb.append(batteryName);
    return sb.toString();
}