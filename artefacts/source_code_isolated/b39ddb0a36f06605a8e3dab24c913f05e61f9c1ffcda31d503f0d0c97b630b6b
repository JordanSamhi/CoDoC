public final Bundle readBundle(@Nullable ClassLoader loader){
    int length = readInt();
    if (length < 0) {
        if (Bundle.DEBUG)
            Log.d(TAG, "null bundle: length=" + length);
        return null;
    }
    final Bundle bundle = new Bundle(this, length);
    if (loader != null) {
        bundle.setClassLoader(loader);
    }
    return bundle;
}