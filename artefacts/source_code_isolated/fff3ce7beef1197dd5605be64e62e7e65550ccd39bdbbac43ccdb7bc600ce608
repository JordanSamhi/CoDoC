public boolean equals(Object obj){
    if (this == obj)
        return true;
    if (obj == null)
        return false;
    if (getClass() != obj.getClass())
        return false;
    NdefRecord other = (NdefRecord) obj;
    if (!Arrays.equals(mId, other.mId))
        return false;
    if (!Arrays.equals(mPayload, other.mPayload))
        return false;
    if (mTnf != other.mTnf)
        return false;
    return Arrays.equals(mType, other.mType);
}