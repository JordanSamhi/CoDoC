public final Activity waitForActivityWithTimeout(long timeOut){
    synchronized (this) {
        if (mLastActivity == null) {
            try {
                wait(timeOut);
            } catch (InterruptedException e) {
            }
        }
        if (mLastActivity == null) {
            return null;
        } else {
            Activity res = mLastActivity;
            mLastActivity = null;
            return res;
        }
    }
}