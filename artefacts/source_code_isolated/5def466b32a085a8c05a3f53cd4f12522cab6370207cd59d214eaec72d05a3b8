public ArrayList<T> getParcelableArrayList(@Nullable String key){
    unparcel();
    Object o = mMap.get(key);
    if (o == null) {
        return null;
    }
    try {
        return (ArrayList<T>) o;
    } catch (ClassCastException e) {
        typeWarning(key, o, "ArrayList", e);
        return null;
    }
}