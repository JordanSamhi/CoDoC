public static String quoteNameIfNecessary(String name){
    int len = name.length();
    for (int i = 0; i < len; i++) {
        char c = name.charAt(i);
        if (!((c >= 'A' && c <= 'Z') || (c >= 'a' && c <= 'z') || (c == ' ') || (c >= '0' && c <= '9'))) {
            return '"' + quoteName(name) + '"';
        }
    }
    return name;
}