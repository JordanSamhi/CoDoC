public ArrayList<String> getHostsList(){
    ArrayList<String> result = new ArrayList<>();
    Iterator<IntentFilter.AuthorityEntry> it = authoritiesIterator();
    if (it != null) {
        while (it.hasNext()) {
            IntentFilter.AuthorityEntry entry = it.next();
            result.add(entry.getHost());
        }
    }
    return result;
}