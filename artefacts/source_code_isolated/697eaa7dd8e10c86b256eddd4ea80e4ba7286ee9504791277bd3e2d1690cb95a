public MotionRange getMotionRange(int axis, int source){
    final int numRanges = mMotionRanges.size();
    for (int i = 0; i < numRanges; i++) {
        final MotionRange range = mMotionRanges.get(i);
        if (range.mAxis == axis && range.mSource == source) {
            return range;
        }
    }
    return null;
}