public int getLowDischargeAmountSinceCharge(){
    synchronized (this) {
        int val = mLowDischargeAmountSinceCharge;
        if (mOnBattery && mDischargeCurrentLevel < mDischargeUnplugLevel) {
            val += mDischargeUnplugLevel - mDischargeCurrentLevel - 1;
        }
        return val;
    }
}