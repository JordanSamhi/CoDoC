public int getVolume(){
    if (mPlaybackType == PLAYBACK_TYPE_LOCAL) {
        int vol = 0;
        try {
            vol = sStatic.mAudioService.getStreamVolume(mPlaybackStream);
        } catch (RemoteException e) {
            Log.e(TAG, "Error getting local stream volume", e);
        }
        return vol;
    } else {
        return mVolume;
    }
}