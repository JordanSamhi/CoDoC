public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    PrintJob other = (PrintJob) obj;
    return (mCachedInfo.getId().equals(other.mCachedInfo.getId()));
}