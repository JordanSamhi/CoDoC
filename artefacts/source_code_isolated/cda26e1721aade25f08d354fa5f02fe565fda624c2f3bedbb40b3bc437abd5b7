public boolean equals(Object other){
    if (other == this)
        return true;
    if (!(other instanceof LocaleList))
        return false;
    final Locale[] otherList = ((LocaleList) other).mList;
    if (mList.length != otherList.length)
        return false;
    for (int i = 0; i < mList.length; i++) {
        if (!mList[i].equals(otherList[i]))
            return false;
    }
    return true;
}