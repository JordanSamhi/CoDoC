public int getInt(int index, int defValue){
    String s = getString(index);
    try {
        return convertValueToInt(s, defValue);
    } catch (NumberFormatException e) {
        Bridge.getLog().warning(LayoutLog.TAG_RESOURCES_FORMAT, String.format("\"%1$s\" in attribute \"%2$s\" is not a valid integer", s, mNames[index]), null, null);
    }
    return defValue;
}