public boolean setActiveProfileOwner(@NonNull ComponentName admin, @Deprecated String ownerName) throws IllegalArgumentException{
    throwIfParentInstance("setActiveProfileOwner");
    if (mService != null) {
        try {
            final int myUserId = myUserId();
            mService.setActiveAdmin(admin, false, myUserId);
            return mService.setProfileOwner(admin, ownerName, myUserId);
        } catch (RemoteException re) {
            throw re.rethrowFromSystemServer();
        }
    }
    return false;
}