public void reset(){
    ensureAnimatorSet();
    if (DBG_ANIMATION_VECTOR_DRAWABLE) {
        Log.w(LOGTAG, "calling reset on AVD: " + ((VectorDrawable.VectorDrawableState) ((AnimatedVectorDrawableState) getConstantState()).mVectorDrawable.getConstantState()).mRootName + ", at: " + this);
    }
    mAnimatorSet.reset();
}