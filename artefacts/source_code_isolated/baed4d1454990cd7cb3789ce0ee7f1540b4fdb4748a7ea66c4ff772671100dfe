public void setCurrentTab(int index){
    if (index < 0 || index >= getTabCount() || index == mSelectedTab) {
        return;
    }
    if (mSelectedTab != -1) {
        getChildTabViewAt(mSelectedTab).setSelected(false);
    }
    mSelectedTab = index;
    getChildTabViewAt(mSelectedTab).setSelected(true);
    mStripMoved = true;
}