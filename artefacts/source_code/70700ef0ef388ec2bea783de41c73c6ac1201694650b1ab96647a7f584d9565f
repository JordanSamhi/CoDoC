public void startAnimations(ValueAnimator.AnimatorUpdateListener listener){
    for (int i = 0; i < mNeedToStart.size(); i++) {
        ObjectAnimator anim = mNeedToStart.get(i);
        anim.addUpdateListener(listener);
        anim.addListener(this);
        anim.start();
    }
    mNeedToStart.clear();
}