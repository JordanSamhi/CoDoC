public boolean dispatchKeyEvent(KeyEvent event){
    if (!mInteractive) {
        if (mDebug)
            Slog.v(TAG, "Waking up on keyEvent");
        wakeUp();
        return true;
    } else if (event.getKeyCode() == KeyEvent.KEYCODE_BACK) {
        if (mDebug)
            Slog.v(TAG, "Waking up on back key");
        wakeUp();
        return true;
    }
    return mWindow.superDispatchKeyEvent(event);
}