public int[] getRtpPayloadTypes(){
    int[] types = new int[mFormats.size()];
    int length = 0;
    for (String format : mFormats) {
        try {
            types[length] = Integer.parseInt(format);
            ++length;
        } catch (NumberFormatException e) {
        }
    }
    return Arrays.copyOf(types, length);
}