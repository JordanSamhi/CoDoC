public static String listToString(List<T> list){
    if (list == null) {
        return null;
    }
    StringBuilder sb = new StringBuilder();
    sb.append('[');
    int size = list.size();
    int i = 0;
    for (T elem : list) {
        sb.append(elem);
        if (i != size - 1) {
            sb.append(',');
        }
        i++;
    }
    sb.append(']');
    return sb.toString();
}