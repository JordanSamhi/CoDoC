public float getFloat(int index, float defValue){
    String s = getString(index);
    try {
        if (s != null) {
            return Float.parseFloat(s);
        }
    } catch (NumberFormatException e) {
        Bridge.getLog().warning(LayoutLog.TAG_RESOURCES_FORMAT, String.format("\"%1$s\" in attribute \"%2$s\" cannot be converted to float.", s, mNames[index]), null, null);
    }
    return defValue;
}