public PersistableBundle getPersistableBundle(@Nullable String key){
    unparcel();
    Object o = mMap.get(key);
    if (o == null) {
        return null;
    }
    try {
        return (PersistableBundle) o;
    } catch (ClassCastException e) {
        typeWarning(key, o, "Bundle", e);
        return null;
    }
}