public static Type createX(RenderScript rs, Element e, int dimX){
    if (dimX < 1) {
        throw new RSInvalidStateException("Dimension must be >= 1.");
    }
    long id = rs.nTypeCreate(e.getID(rs), dimX, 0, 0, false, false, 0);
    Type t = new Type(id, rs);
    t.mElement = e;
    t.mDimX = dimX;
    t.calcElementCount();
    return t;
}