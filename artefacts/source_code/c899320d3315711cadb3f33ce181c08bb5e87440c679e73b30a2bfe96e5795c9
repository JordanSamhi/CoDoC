public void readFromParcel(Parcel source){
    mId = source.readString();
    mReason = source.readString();
    mResInstanceState = source.readInt();
    mPresentityUri = source.readString();
    Parcelable[] tempParcelableArray = source.readParcelableArray(PresTupleInfo.class.getClassLoader());
    mTupleInfoArray = new PresTupleInfo[] {};
    if (tempParcelableArray != null) {
        mTupleInfoArray = Arrays.copyOf(tempParcelableArray, tempParcelableArray.length, PresTupleInfo[].class);
    }
}