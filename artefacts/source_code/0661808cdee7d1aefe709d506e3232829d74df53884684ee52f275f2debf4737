public static PorterDuff.Mode parseTintMode(int value, Mode defaultMode){
    switch(value) {
        case 3:
            return Mode.SRC_OVER;
        case 5:
            return Mode.SRC_IN;
        case 9:
            return Mode.SRC_ATOP;
        case 14:
            return Mode.MULTIPLY;
        case 15:
            return Mode.SCREEN;
        case 16:
            return Mode.ADD;
        default:
            return defaultMode;
    }
}