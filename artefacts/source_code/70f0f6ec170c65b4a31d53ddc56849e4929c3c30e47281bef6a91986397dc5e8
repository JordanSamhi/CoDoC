public String toString(){
    StringBuilder sb = new StringBuilder("TonemapCurve{");
    sb.append("R:");
    sb.append(curveToString(CHANNEL_RED));
    sb.append(", G:");
    sb.append(curveToString(CHANNEL_GREEN));
    sb.append(", B:");
    sb.append(curveToString(CHANNEL_BLUE));
    sb.append("}");
    return sb.toString();
}