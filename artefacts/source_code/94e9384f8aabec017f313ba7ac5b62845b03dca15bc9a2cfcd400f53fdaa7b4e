public byte lookupScore(int rssi, boolean isActiveNetwork){
    if (isActiveNetwork) {
        rssi += activeNetworkRssiBoost;
    }
    int index = (rssi - start) / bucketWidth;
    if (index < 0) {
        index = 0;
    } else if (index > rssiBuckets.length - 1) {
        index = rssiBuckets.length - 1;
    }
    return rssiBuckets[index];
}