public boolean swipeRight(int steps) throws UiObjectNotFoundException{
    Tracer.trace(steps);
    Rect rect = getVisibleBounds();
    if (rect.width() <= SWIPE_MARGIN_LIMIT * 2)
        return false;
    return getInteractionController().swipe(rect.left + SWIPE_MARGIN_LIMIT, rect.centerY(), rect.right - SWIPE_MARGIN_LIMIT, rect.centerY(), steps);
}