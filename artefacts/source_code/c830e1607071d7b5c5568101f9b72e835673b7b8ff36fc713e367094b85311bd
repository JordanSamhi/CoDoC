public static String stateToString(int state){
    switch(state) {
        case STATE_CREATED:
            {
                return "STATE_CREATED";
            }
        case STATE_QUEUED:
            {
                return "STATE_QUEUED";
            }
        case STATE_STARTED:
            {
                return "STATE_STARTED";
            }
        case STATE_BLOCKED:
            {
                return "STATE_BLOCKED";
            }
        case STATE_FAILED:
            {
                return "STATE_FAILED";
            }
        case STATE_COMPLETED:
            {
                return "STATE_COMPLETED";
            }
        case STATE_CANCELED:
            {
                return "STATE_CANCELED";
            }
        default:
            {
                return "STATE_UNKNOWN";
            }
    }
}