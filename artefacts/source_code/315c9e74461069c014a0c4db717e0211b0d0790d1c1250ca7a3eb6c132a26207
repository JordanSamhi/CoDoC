public static String setToCheckinString(int set){
    switch(set) {
        case SET_ALL:
            return "all";
        case SET_DEFAULT:
            return "def";
        case SET_FOREGROUND:
            return "fg";
        case SET_DBG_VPN_IN:
            return "vpnin";
        case SET_DBG_VPN_OUT:
            return "vpnout";
        default:
            return "unk";
    }
}