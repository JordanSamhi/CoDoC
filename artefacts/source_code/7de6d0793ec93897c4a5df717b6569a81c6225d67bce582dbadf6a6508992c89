public boolean right(){
    if (mColumn == 6) {
        mRow++;
        mColumn = 0;
    } else {
        mColumn++;
    }
    if (isWithinCurrentMonth(mRow, mColumn)) {
        return false;
    }
    nextMonth();
    mRow = 0;
    mColumn = 0;
    while (!isWithinCurrentMonth(mRow, mColumn)) {
        mColumn++;
    }
    return true;
}