public boolean hideOverflowMenu(){
    if (mPostedOpenRunnable != null && mMenuView != null) {
        ((View) mMenuView).removeCallbacks(mPostedOpenRunnable);
        mPostedOpenRunnable = null;
        return true;
    }
    MenuPopupHelper popup = mOverflowPopup;
    if (popup != null) {
        popup.dismiss();
        return true;
    }
    return false;
}