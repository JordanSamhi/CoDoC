public final void writeParcelableArray(@Nullable T[] value, int parcelableFlags){
    if (value != null) {
        int N = value.length;
        writeInt(N);
        for (int i = 0; i < N; i++) {
            writeParcelable(value[i], parcelableFlags);
        }
    } else {
        writeInt(-1);
    }
}