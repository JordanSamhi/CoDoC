public boolean setRadioPower(boolean turnOn){
    try {
        ITelephony telephony = getITelephony();
        if (telephony != null)
            return telephony.setRadioPower(turnOn);
    } catch (RemoteException e) {
        Log.e(TAG, "Error calling ITelephony#setRadioPower", e);
    }
    return false;
}