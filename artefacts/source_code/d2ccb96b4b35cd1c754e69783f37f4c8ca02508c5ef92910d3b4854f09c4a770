public static Rating newUnratedRating(@Style int ratingStyle){
    switch(ratingStyle) {
        case RATING_HEART:
        case RATING_THUMB_UP_DOWN:
        case RATING_3_STARS:
        case RATING_4_STARS:
        case RATING_5_STARS:
        case RATING_PERCENTAGE:
            return new Rating(ratingStyle, RATING_NOT_RATED);
        default:
            return null;
    }
}