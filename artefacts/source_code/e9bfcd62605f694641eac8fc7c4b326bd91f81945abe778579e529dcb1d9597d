public int redo(UndoOwner[] owners, int count){
    if (mWorking != null) {
        throw new IllegalStateException("Can't be called during an update");
    }
    int num = 0;
    int i = -1;
    mInUndo = true;
    while (count > 0 && (i = findPrevState(mRedos, owners, i)) >= 0) {
        UndoState state = mRedos.remove(i);
        state.redo();
        mUndos.add(state);
        count--;
        num++;
    }
    mInUndo = false;
    return num;
}