public void abortReliableWrite(){
    if (VDBG)
        Log.d(TAG, "abortReliableWrite() - device: " + mDevice.getAddress());
    if (mService == null || mClientIf == 0)
        return;
    try {
        mService.endReliableWrite(mClientIf, mDevice.getAddress(), false);
    } catch (RemoteException e) {
        Log.e(TAG, "", e);
    }
}