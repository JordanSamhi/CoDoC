public boolean equals(final Object obj){
    if (obj == null) {
        return false;
    } else if (this == obj) {
        return true;
    } else if (obj instanceof RggbChannelVector) {
        final RggbChannelVector other = (RggbChannelVector) obj;
        return mRed == other.mRed && mGreenEven == other.mGreenEven && mGreenOdd == other.mGreenOdd && mBlue == other.mBlue;
    }
    return false;
}