public short get(int i){
    switch(i) {
        case 0:
            return (short) (x);
        case 1:
            return (short) (y);
        case 2:
            return (short) (z);
        case 3:
            return (short) (w);
        default:
            throw new IndexOutOfBoundsException("Index: i");
    }
}