public boolean hostedByPackageForUser(String packageName, int userId){
    final int N = widgets.size();
    for (int i = 0; i < N; i++) {
        Widget widget = widgets.get(i);
        if (packageName.equals(widget.host.id.packageName) && widget.host.getUserId() == userId) {
            return true;
        }
    }
    return false;
}