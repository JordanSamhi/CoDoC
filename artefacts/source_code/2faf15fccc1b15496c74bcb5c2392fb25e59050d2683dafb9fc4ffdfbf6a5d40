public SparseArray<T> getSparseParcelableArray(@Nullable String key){
    unparcel();
    Object o = mMap.get(key);
    if (o == null) {
        return null;
    }
    try {
        return (SparseArray<T>) o;
    } catch (ClassCastException e) {
        typeWarning(key, o, "SparseArray", e);
        return null;
    }
}