public void send(int resultCode, Bundle resultData){
    if (mLocal) {
        if (mHandler != null) {
            mHandler.post(new MyRunnable(resultCode, resultData));
        } else {
            onReceiveResult(resultCode, resultData);
        }
        return;
    }
    if (mReceiver != null) {
        try {
            mReceiver.send(resultCode, resultData);
        } catch (RemoteException e) {
        }
    }
}