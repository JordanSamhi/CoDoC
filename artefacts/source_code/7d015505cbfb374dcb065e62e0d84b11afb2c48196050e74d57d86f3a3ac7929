public List<Call> getChildren(){
    if (!mChildrenCached) {
        mChildrenCached = true;
        mChildren.clear();
        for (String id : mChildrenIds) {
            Call call = mPhone.internalGetCallByTelecomId(id);
            if (call == null) {
                mChildrenCached = false;
            } else {
                mChildren.add(call);
            }
        }
    }
    return mUnmodifiableChildren;
}