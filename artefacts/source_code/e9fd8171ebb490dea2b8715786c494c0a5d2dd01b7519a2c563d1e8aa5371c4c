public ExpandableView getLastChildNotGone(){
    int childCount = getChildCount();
    for (int i = childCount - 1; i >= 0; i--) {
        View child = getChildAt(i);
        if (child.getVisibility() != View.GONE && child != mShelf) {
            return (ExpandableView) child;
        }
    }
    return null;
}