public final void readStringList(@NonNull List<String> list){
    int M = list.size();
    int N = readInt();
    int i = 0;
    for (; i < M && i < N; i++) {
        list.set(i, readString());
    }
    for (; i < N; i++) {
        list.add(readString());
    }
    for (; i < M; i++) {
        list.remove(N);
    }
}