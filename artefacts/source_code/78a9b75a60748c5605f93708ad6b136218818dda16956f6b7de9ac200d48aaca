public void setValues(boolean[] values){
    if (mSetValues != null) {
        Arrays.fill(mSetValues, false);
        Arrays.fill(mOrigValues, false);
        if (values != null) {
            System.arraycopy(values, 0, mSetValues, 0, values.length < mSetValues.length ? values.length : mSetValues.length);
        }
    }
}