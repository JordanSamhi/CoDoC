public boolean longTapNoSync(int x, int y){
    if (DEBUG) {
        Log.d(LOG_TAG, "longTapNoSync (" + x + ", " + y + ")");
    }
    if (touchDown(x, y)) {
        SystemClock.sleep(mUiAutomatorBridge.getSystemLongPressTime());
        if (touchUp(x, y)) {
            return true;
        }
    }
    return false;
}