public final void revokeTrust(){
    synchronized (mLock) {
        if (mPendingGrantTrustTask != null) {
            mPendingGrantTrustTask = null;
        }
        if (mCallback != null) {
            try {
                mCallback.revokeTrust();
            } catch (RemoteException e) {
                onError("calling revokeTrust()");
            }
        }
    }
}