public void reset(){
    mDisabled = !Log.isLoggable(mTag, Log.VERBOSE);
    if (mDisabled)
        return;
    if (mSplits == null) {
        mSplits = new ArrayList<Long>();
        mSplitLabels = new ArrayList<String>();
    } else {
        mSplits.clear();
        mSplitLabels.clear();
    }
    addSplit(null);
}