public boolean isOtherSeqNewer(Configuration other){
    if (other == null) {
        return false;
    }
    if (other.seq == 0) {
        return true;
    }
    if (seq == 0) {
        return true;
    }
    int diff = other.seq - seq;
    if (diff > 0x10000) {
        return false;
    }
    return diff > 0;
}