public String getType(){
    switch(mNdefType) {
        case TYPE_1:
            return NFC_FORUM_TYPE_1;
        case TYPE_2:
            return NFC_FORUM_TYPE_2;
        case TYPE_3:
            return NFC_FORUM_TYPE_3;
        case TYPE_4:
            return NFC_FORUM_TYPE_4;
        case TYPE_MIFARE_CLASSIC:
            return MIFARE_CLASSIC;
        case TYPE_ICODE_SLI:
            return ICODE_SLI;
        default:
            return UNKNOWN;
    }
}