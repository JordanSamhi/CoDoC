public boolean supportsUriScheme(String uriScheme){
    if (mSupportedUriSchemes == null || uriScheme == null) {
        return false;
    }
    for (String scheme : mSupportedUriSchemes) {
        if (scheme != null && scheme.equals(uriScheme)) {
            return true;
        }
    }
    return false;
}