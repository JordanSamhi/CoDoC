public static int getPrimitiveTypeSize(int nativeType){
    switch(nativeType) {
        case TYPE_BYTE:
            return SIZEOF_BYTE;
        case TYPE_INT32:
            return SIZEOF_INT32;
        case TYPE_FLOAT:
            return SIZEOF_FLOAT;
        case TYPE_INT64:
            return SIZEOF_INT64;
        case TYPE_DOUBLE:
            return SIZEOF_DOUBLE;
        case TYPE_RATIONAL:
            return SIZEOF_RATIONAL;
    }
    throw new UnsupportedOperationException("Unknown type, can't get size for " + nativeType);
}