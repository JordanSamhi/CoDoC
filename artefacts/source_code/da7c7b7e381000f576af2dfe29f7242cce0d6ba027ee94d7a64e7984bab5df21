public static float getFinalTranslationY(View view){
    if (view == null) {
        return 0;
    }
    ValueAnimator yAnimator = getChildTag(view, TAG_ANIMATOR_TRANSLATION_Y);
    if (yAnimator == null) {
        return view.getTranslationY();
    } else {
        return getChildTag(view, TAG_END_TRANSLATION_Y);
    }
}