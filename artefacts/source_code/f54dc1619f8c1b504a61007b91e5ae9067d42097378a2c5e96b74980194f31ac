public int compareTo(Rational another){
    checkNotNull(another, "another must not be null");
    if (equals(another)) {
        return 0;
    } else if (isNaN()) {
        return 1;
    } else if (another.isNaN()) {
        return -1;
    } else if (isPosInf() || another.isNegInf()) {
        return 1;
    } else if (isNegInf() || another.isPosInf()) {
        return -1;
    }
    long thisNumerator = ((long) mNumerator) * another.mDenominator;
    long otherNumerator = ((long) another.mNumerator) * mDenominator;
    if (thisNumerator < otherNumerator) {
        return -1;
    } else if (thisNumerator > otherNumerator) {
        return 1;
    } else {
        return 0;
    }
}