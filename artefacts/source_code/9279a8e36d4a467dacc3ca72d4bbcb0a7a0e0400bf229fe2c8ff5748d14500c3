public static String setToString(int set){
    switch(set) {
        case SET_ALL:
            return "ALL";
        case SET_DEFAULT:
            return "DEFAULT";
        case SET_FOREGROUND:
            return "FOREGROUND";
        case SET_DBG_VPN_IN:
            return "DBG_VPN_IN";
        case SET_DBG_VPN_OUT:
            return "DBG_VPN_OUT";
        default:
            return "UNKNOWN";
    }
}