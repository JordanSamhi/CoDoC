public static T getOrDefault(CaptureRequest r, CaptureRequest.Key<T> key, T defaultValue){
    checkNotNull(r, "r must not be null");
    checkNotNull(key, "key must not be null");
    checkNotNull(defaultValue, "defaultValue must not be null");
    T value = r.get(key);
    if (value == null) {
        return defaultValue;
    } else {
        return value;
    }
}