public boolean canTransfer(Call heldCall){
    Phone activePhone = null;
    Phone heldPhone = null;
    if (hasActiveFgCall()) {
        activePhone = getActiveFgCall().getPhone();
    }
    if (heldCall != null) {
        heldPhone = heldCall.getPhone();
    }
    return (heldPhone == activePhone && activePhone.canTransfer());
}