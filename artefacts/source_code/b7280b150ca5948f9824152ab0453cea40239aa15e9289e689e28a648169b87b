public String flatten(){
    StringBuilder flattened = new StringBuilder(128);
    for (String k : mMap.keySet()) {
        flattened.append(k);
        flattened.append("=");
        flattened.append(mMap.get(k));
        flattened.append(";");
    }
    flattened.deleteCharAt(flattened.length() - 1);
    return flattened.toString();
}