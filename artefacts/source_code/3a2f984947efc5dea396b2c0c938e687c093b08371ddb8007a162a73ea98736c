public ArrayList<Animator> getChildAnimations(){
    ArrayList<Animator> childList = new ArrayList<Animator>();
    int size = mNodes.size();
    for (int i = 0; i < size; i++) {
        Node node = mNodes.get(i);
        if (node != mRootNode) {
            childList.add(node.mAnimation);
        }
    }
    return childList;
}