public static int getBytesPerMessage(byte statusByte){
    int statusInt = statusByte & 0xFF;
    if (statusInt >= 0xF0) {
        return SYSTEM_BYTE_LENGTHS[statusInt & 0x0F];
    } else if (statusInt >= 0x80) {
        return CHANNEL_BYTE_LENGTHS[(statusInt >> 4) - 8];
    } else {
        return 0;
    }
}